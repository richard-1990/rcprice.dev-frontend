{
  "name": "front-end",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "test:ci": "ng test -- --watch=false --code-coverage --browsers=ChromeHeadless",
    "test:headless": "ng test --code-coverage --browsers=ChromeHeadless",
    "lint": "eslint \"src/**/*.{js,ts}\"",
    "lint:fix": "eslint \"src/**/*.{js,ts}\" --fix",
    "lint:ci": "eslint \"src/**/*.{js,ts}\" --quiet",
    "prettier": "prettier --write 'src/**/*.{ts,js,scss}'",
    "e2e": "ng e2e",
    "dev:ssr": "ng run front-end:serve-ssr",
    "serve:ssr": "node dist/front-end/server/main.js",
    "build:ssr": "ng build --prod && ng run front-end:server:production",
    "prerender": "ng run front-end:prerender",
    "git:delete-merged": "git branch --merged | egrep -v \"(^\\*|master|develop)\" | xargs git branch -d"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^10.2.0",
    "@angular/cdk": "^10.2.5",
    "@angular/common": "^10.2.0",
    "@angular/compiler": "^10.2.0",
    "@angular/core": "^10.2.0",
    "@angular/fire": "^6.0.3",
    "@angular/forms": "^10.2.0",
    "@angular/material": "^10.2.5",
    "@angular/platform-browser": "^10.2.0",
    "@angular/platform-browser-dynamic": "^10.2.0",
    "@angular/platform-server": "^10.2.0",
    "@angular/router": "^10.2.0",
    "@fortawesome/angular-fontawesome": "^0.7.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.35",
    "@fortawesome/free-brands-svg-icons": "^5.15.3",
    "@fortawesome/free-regular-svg-icons": "^5.15.3",
    "@fortawesome/free-solid-svg-icons": "^5.15.3",
    "@nguniversal/express-engine": "^10.1.0",
    "express": "^4.15.2",
    "firebase": "^7.21.1",
    "highlight.js": "^10.7.1",
    "ngx-build-plus": "^10.1.1",
    "ngx-highlightjs": "^4.1.3",

    "ngx-quill": "^12.0.1",
    "quill": "^1.3.7",
    "rxjs": "^6.6.7",
    "tslib": "^2.1.0",
    "zone.js": "^0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.1001.7",
    "@angular/cli": "^11.2.11",
    "@angular/compiler-cli": "^10.2.0",

    "@angular/language-service": "^10.2.3",
    "@nguniversal/builders": "^10.1.0",
    "@types/express": "^4.17.11",
    "@types/jasmine": "^3.6.9",

    "@types/jasminewd2": "^2.0.3",
    "@types/node": "^14.14.35",
    "@typescript-eslint/eslint-plugin": "^2.34.0",
    "@typescript-eslint/parser": "^2.34.0",
    "codelyzer": "^6.0.1",
    "eslint": "^6.8.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-prettier": "^3.3.1",
    "jasmine-core": "^3.7.0",
    "jasmine-spec-reporter": "^5.0.0",
    "karma": "^5.2.3",
    "karma-chrome-launcher": "^3.1.0",
    "karma-coverage-istanbul-reporter": "^3.0.2",
    "karma-jasmine": "^4.0.1",
    "karma-jasmine-html-reporter": "^1.5.0",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",

    "postcss-scss": "^3.0.5",
    "prettier": "^2.2.1",
    "prettier-eslint": "^11.0.0",
    "protractor": "^7.0.0",
    "tailwindcss": "^1.9.6",
    "ts-node": "^9.1.1",
    "tslint": "^6.1.3",
    "typescript": "^3.9.7"
  }
}
